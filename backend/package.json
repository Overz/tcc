{
  "name": "tcc",
  "version": "1.0.0",
  "description": "Tcc senac 01/2021",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "start": "node build/index.js",
    "dev": "ts-node-dev src/index.ts --poll --transpile-only --ignore-watch node_modules",
    "build": "npm run clean && tsc -p tsconfig.prod.json --removeComments",
    "test": "jest",
    "watch": "jest --watchAll --no-cache",
    "style": "prettier --check 'src/**/*.ts'",
    "lint": "eslint --max-warnings=0",
    "coverage": "jest --coverage",
    "check": "npm run style && npm run lint && npm run coverage",
    "orm": "ts-node ./node_modules/typeorm/cli.js",
    "db:migrate": "npm run orm migration:run",
    "db:drop": "npm run orm schema:drop",
    "db:revert": "npm run orm migration:revert",
    "db:create": "npm run orm migration:create -- -n $1",
    "db:inserts": "npx ts-node ./src/tests/generator/inserts.ts",
    "db:retry": "npm run db:drop && npm run db:inserts && npm run dataset",
    "dataset": "npx ts-node ./src/tests/generator/dataset.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://tcc-senac-2021@dev.azure.com/tcc-senac-2021/TCC/_git/backend"
  },
  "keywords": [
    "tcc",
    "senac",
    "backend"
  ],
  "author": "Christian Lima",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.2",
    "bcrypt": "5.0.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cpf-cnpj-validator": "1.0.3",
    "env-var": "7.0.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-validator": "6.9.2",
    "jsonwebtoken": "8.5.1",
    "ms": "2.0.0",
    "multer": "1.4.2",
    "mysql": "^2.14.1",
    "nanoid": "3.1.31",
    "nodemailer": "6.5.0",
    "pg": "8.6.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.30",
    "umzug": "2.3.0",
    "validations-br": "1.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@types/axios": "0.14.0",
    "@types/bcrypt": "3.0.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.10",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "8.5.0",
    "@types/ms": "0.7.31",
    "@types/multer": "1.4.5",
    "@types/nanoid": "2.1.0",
    "@types/node": "15.6.1",
    "@types/nodemailer": "6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/umzug": "2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "dotenv": "8.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.10.1",
    "prettier": "2.2.1",
    "sqlite3": "5.0.2",
    "supertest": "^6.1.1",
    "ts-jest": "26.5.3",
    "ts-node-dev": "1.1.1",
    "typescript": "4.2.3"
  }
}
